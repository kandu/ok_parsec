(jbuild_version 1)

(library
 ((name ok_parsec)
  (public_name ok_parsec)
  (flags (:standard -safe-string))
  (synopsis "a monadic parser combinator library that deal with data of unpredictable length concurrently")
  (libraries (core_kernel ocamlgraph lwt.unix))
  (preprocess (per_module
    ((pps (lwt.ppx)) (common))
    ((pps (ppx_ok_monad)) (re parsec))
    ))
  ))

